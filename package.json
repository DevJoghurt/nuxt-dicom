{
  "name": "@nuxthealth/dicom",
  "version": "0.0.10",
  "description": "Nuxt module for DICOM",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DevJoghurt/nuxt-dicom.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn dev:prepare && nuxt-module-build build",
    "prepack": "yarn build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bumpp && npm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "cleanup": "rimraf 'playground/node_modules' 'node_modules'",
    "bumpp": "bumpp"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "@nuxthealth/node-dicom": "0.0.8",
    "pm2": "^5.4.2",
    "zod": "^3.23.8",
    "@nuxt/ui": "^2.18.4",
    "@iconify-json/heroicons": "1.1.24",
    "read-last-lines": "^1.8.0",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.4.0",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "bumpp": "9.4.1",
    "@types/node": "^22.0.3",
    "changelogen": "^0.5.5",
    "eslint": "^9.8.0",
    "nuxt": "^3.12.4",
    "typescript": "latest",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29",
    "rimraf": "^6.0.1"
  }
}
